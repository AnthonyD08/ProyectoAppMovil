<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BASE.MenuFlyout" BackgroundColor="#4FD190"
             xmlns:pages="clr-namespace:BASE">
  <FlyoutPage.Flyout>
        <pages:MenuFlyoutFlyout x:Name="FlyoutPage" BackgroundColor="#4FD190"/>
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:MenuFlyoutDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>